{
  "_version": "1.8.0",
  "sap.app": {
    "id": "rroot",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "flight": {
        "uri": "https://s4c.leverx.local:44300/sap/opu/odata/sap/ZZPRACTISE_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "rroot.view.Overview",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.30",
      "libs": {
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "rroot.i18n.i18n"
        }
      },
      "oData": {
        "dataSource": "flight"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "rroot.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "login",
          "target": "login"
        },
        {
          "pattern": "application",
          "name": "application",
          "target": "application"
        },
        {
          "pattern": "content",
          "name": "content",
          "target": "content"
        }
      ],
      "targets": {
        "login": {
          "viewID": "login",
          "viewName": "login"
        },
        "application": {
          "viewId": "application",
          "viewName": "Application"
        },
        "content": {
          "viewId": "content",
          "viewName": "Content"
        }
      }
    }
  }
}